{
  "name": "sabre-legacy-client",
  "version": "0.1.10",
  "description": "Sabre Legacy Node.js SDK",
  "keywords": [
    "sabre",
    "legacy",
    "client",
    "soap"
  ],
  "homepage": "https://github.com/CarlosIvanSoto/sabre-legacy#readme",
  "bugs": {
    "url": "https://github.com/CarlosIvanSoto/sabre-legacy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CarlosIvanSoto/sabre-legacy.git"
  },
  "license": "MIT",
  "author": "Carlos Ivan Soto <csoto_99@outlook.com>",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "link": "npm run build & npm link",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.3",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  },
  "dependencies": {
    "fast-xml-parser": "^4.5.1"
  }
}
